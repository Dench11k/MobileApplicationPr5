<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="57dp"
        android:layout_height="68dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="14dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="28dp"
        android:src="@drawable/zanchok"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.087" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="95dp"
        android:layout_height="42dp"
        android:layout_below="@+id/textView11"
        android:layout_marginStart="41dp"
        android:layout_marginTop="1dp"
        android:layout_toEndOf="@+id/imageView3"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="@dimen/text_size2"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.297"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="95dp"
        android:layout_height="42dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="18dp"
        android:layout_toEndOf="@+id/imageView3"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="@dimen/text_size2"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.297"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="178dp"
        android:layout_height="157dp"
        android:src="@drawable/_fc08dc1_fd63_40cf_bdd3_9b39e6f70ae3" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView3"
        android:layout_marginStart="-113dp"
        android:layout_marginLeft="-113dp"
        android:layout_marginTop="-11dp"
        android:layout_toEndOf="@+id/imageView3"
        android:layout_toRightOf="@id/imageView3"
        android:text="LADA"
        android:textColor="@color/black"
        android:textSize="@dimen/text_size2" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="406dp"
        android:layout_height="100dp"
        android:layout_below="@+id/imageView3"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="3dp"
        android:gravity="center"
        android:text="Автомобили"
        android:textColor="@color/black"
        android:textSize="20pt" />


    <EditText
        android:id="@+id/editTextCar"
        android:layout_width="309dp"
        android:layout_height="50dp"
        android:layout_below="@+id/button4"
        android:layout_alignParentStart="true"
        android:layout_marginStart="62dp"
        android:layout_marginTop="96dp"
        android:ems="15"
        android:gravity="center"
        android:hint="Выбранный автомобиль"
        android:inputType="textPersonName"
        android:textSize="@dimen/text_size2" />

    <Button
        android:id="@+id/button2"
        android:layout_width="151dp"
        android:layout_height="52dp"
        android:layout_below="@+id/editTextCar"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="137dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="123dp"
        android:onClick="onMyButton1"
        android:text="Готово" />

    <Button
        android:id="@+id/button4"
        android:layout_width="166dp"
        android:layout_height="81dp"
        android:layout_below="@+id/textView5"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="123dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="122dp"
        android:text="Lada Granta" />

</RelativeLayout>
